{
    "name": "ledjin/sylius-sagepay-bundle",
    "type": "bundle",
    "description": "Sagepay payment lib",
    "keywords": ["payment", "sagepay", "payum", "sylius", "payum bundle", "symfony2", "ledjin sagepay"],
    "homepage": "http://www.ledj.in",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Jeliuc",
            "email": "a2xchip@gmail.com"
        },
        {
            "name":     "LedjIn Labs",
            "homepage": "http://www.ledj.in"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/LedjIn/Sagepay"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:zyleee/Sagepay.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:zyleee/SagepayBundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",

        "sylius/sylius":                "0.14.*@dev",
        "doctrine/migrations":          "~1.0@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "zyleee/sagepay-bundle": "dev-compatibility-payum-0.13-0.14",
        "ledjin/sagepay": "dev-compatibility-payum-0.14"

    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Ledjin\\Bundle\\SyliusSagepayBundle": "" }
    },
    "target-dir": "Ledjin/Bundle/SyliusSagepayBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
